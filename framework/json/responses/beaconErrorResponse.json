    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": true,
    "description": "An unsuccessful operation.",
    "properties": {
        "error": {
            "$ref": "https://raw.githubusercontent.com/MrRobb/beacon-v2/main/framework/json/common/beaconCommonComponents.json#/definitions/BeaconError",
            "description": "Returning an error."
        },
        "meta": {
            "$ref": "https://raw.githubusercontent.com/MrRobb/beacon-v2/main/framework/json/responses/sections/beaconResponseMeta.json",
